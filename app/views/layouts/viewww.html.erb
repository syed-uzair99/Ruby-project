      <% b = Time.parse(m.created_at.to_s) %>
      <td><%= ((a - b) / 3600).round %> hours ago</td>         
    </tr>
    <% end %>
   </table>
  </div>
</div><br>
<div class="container">
  <button type="button" class="btn btn-info" data-toggle="collapse" data-target="#demof"><img src="data:image/jpeg;base64,"  width="75" height="75"></button>
  <div id="demof" class="collapse">
  <table>
    <tr>
      <th>Item</th>              
      <th>Qty</th>                      
      <th>Price</th>
      <th>Purchased</th>
    </tr>
   <% @yy.each do |m| %>
    <tr>
