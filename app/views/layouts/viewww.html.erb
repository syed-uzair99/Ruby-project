</svg><b><%= @b+@c+@f %></b><br>
Total Earnings
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
  <script type="text/javascript">
    google.charts.load('current', {'packages':['corechart']});
    google.charts.setOnLoadCallback(drawChart);

    function drawChart() {

      var data = google.visualization.arrayToDataTable([
        ["Items","Cost"],
        ["Books",<%= @b %>],
        ["Furniture",<%= @f %>],
        ["Clothes",<%= @c %>]
      ]);

      var options = {
        pieHole: 0.75,
        pieSliceTextStyle: {
          color: 'black',
        },
        legend: 'Total Earnings'
      };

      var chart = new google.visualization.PieChart(document.getElementById('donut_single'));
      chart.draw(data, options);
    }
  </script>
  <style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 25%;
}
td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 5px;
}
</style>
</head>
<body>
  <div id="donut_single" style="width: 250px; height: 100px;"></div>
  <b>Stock Details</b> <br> 
  <table>
  <tr>
    <th>Item</th>              
    <th>Sold</th>                      
    <th>Remaining</th>
  </tr>
 <% @stt.each do |m| %>
  <tr>
    <td><%= m.ItemName %></td>         
    <td><%= m.SoldStock %></td> 
    <td><%= m.RemainingStock %></td>      
  </tr>
  <% end %>
 </table> <br>
 <b>Customers</b> - <%= @count %> <br>
 <table>
  <tr>
    <th>Name</th>              
    <th>Email</th> 
    <th>Purchase</th>                     
  </tr>
 <% @u.each do |m| %>
  <tr>
    <td><%= m.name %></td>         
    <td><%= m.email %></td> 
    <% w=@s.where(SaleNo:m.id) %>   
    <td><%= w.sum(:TotalCost) %></td>  
  </tr>
  <% end %>
  </table>
  <br><b>Orders</b><br>
  <div class="container">
  <button type="button" class="btn btn-info" data-toggle="collapse" data-target="#demo"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRKET-Lz1DwPeetCrtTJIaVOVMuA_tWKE7skSP3LAwnjScredtDJMI9zRV_4qsjSe4A5m4&usqp=CAU"  width="75" height="75"></button>
  <div id="demo" class="collapse">
  <table>
    <tr>
      <th>Item</th>              
      <th>Qty</th>                      
      <th>Price</th>
      <th>Purchased</th>
    </tr>
   <% @xx.each do |m| %>
    <tr>
    <td><%= m.ItemName %></td>         
      <td><%= m.TotalQuantity %></td> 
      <td><%= m.TotalCost %></td> 
      <% require 'time' %>
      <% a = Time.parse(DateTime.now.to_s) %>
      <% b = Time.parse(m.created_at.to_s) %>
      <td><%= ((a - b) / 3600).round %> hours ago</td>       
    </tr>
    <% end %>
   </table>
  </div>
</div><br>
<div class="container">
<button type="button" class="btn btn-info" data-toggle="collapse" data-target="#democ"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTQaKIOwzoPBPI3fzdYBp-N2cLwM1XZYy-5Lw&usqp=CAU"  width="75" height="75"></button>
  <div id="democ" class="collapse">
  <table>
    <tr>
      <th>Item</th>              
      <th>Qty</th>                      
      <th>Price</th>
      <th>Purchased</th>
    </tr>
   <% @zz.each do |m| %>
